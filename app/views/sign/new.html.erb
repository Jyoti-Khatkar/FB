<div class="Fluid container" id="topmargin">
<%= form_with url: "/Contentpage", method: :post do |form| %>
  <div class="field form-group mx-sm-3 mb-2">
    
    <%= form.text_field :email, class:"form-control", placeholder:"Name" %>
  </div>
  
  

  <div class="field form-group mx-sm-3 mb-2">
    
    <%= form.text_field :email, class:"form-control", placeholder:"Email" %>
  </div>

  <div class="field form-group mx-sm-3 mb-2">
    
    <%= form.text_field :password, class:"form-control", placeholder:"Password" %>
  </div>
  <br/>
  <div class="actions mx-sm-3 mb-2">
    <%= form.submit 'Done', class:'btn btn-secondary' %>
  </div>
  
  
 
<% end %>